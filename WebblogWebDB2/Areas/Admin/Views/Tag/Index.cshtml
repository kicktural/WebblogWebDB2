@model List<Tag>


  <a href="/Admin/Tag/Create" class="btn btn-info">Create</a>
<table class="table table-dark table-striped table-hover ">
    <thead>
        <tr>
            <th>TagName</th>
            <th>Id</th>
            <th>#</th>
            <th>#</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.TagName</td>
                <td>@item.Id</td>
                <td>
                    <a href="/Admin/Tag/Update/@item.Id" class="btn btn-info">Update</a>
                    <a href="/Admin/Tag/Delete/@item.Id" class="btn btn-danger">Delete</a>
                    <a href="/Admin/Tag/detail/@item.Id" class="btn btn-info">Detail</a>
                </td>
            </tr>
        }
    </tbody>
</table>
